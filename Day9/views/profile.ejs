<!DOCTYPE html>
<html>
<head>
	<title>my website</title>
	<style type="text/css">
		body{
			background-color: aqua;
			padding: 30px;
		}
		h1{
			color:maroon;
			font-size: 80px;
			text-transform: uppercase;
			text-align: center;
		}
		p,h2,ul{
			font-size: 30px;
			color:black;
			text-align: center;
		}
	</style>
</head>
<body>
	<%-include('partials/nav.ejs')%>
	<h1>hello , i am <%=person%></h1>					
	<p><strong>Age:<%=data.age%></strong></p>
	<p><strong>Occupation:<%=data.occupation%></strong></p>
	<h2>Hobbies:</h2>
	<ul>
	<% data.hobbies.forEach(function(view){ %>
	
		<li><%=view%></li>

	<% }); %>
	</ul>
</body>
</html>
